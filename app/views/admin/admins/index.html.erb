<h1 align="center">Admin's Workspace</h1>
<br>
  <table class="table table-hover">
    <thead>
      <tr>
        <th>Username</th>
        <th>E-mail</th>
        <th>Contact#</th>
        <th>Gender</th>
        <th>Role</th>
        <th>Enable / Disable</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
            <td><%= user.username %></td>
            <td><%= user.email %></td>
            <td><%= user.contact %></td>
            <td><%= user.gender %></td>
            <td><%= user.role %></td>
            <td><button class="btn btn-info">Enable</button>    <button class="btn btn-info">Disable</button></td>
        </tr>
      <% end %>
    </tbody>
  </table>
